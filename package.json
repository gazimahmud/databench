{
  "name": "databench",
  "version": "0.4.0-beta.2",
  "description": "Realtime data analysis tool.",
  "main": "js/build/node_client/main.js",
  "directories": {
    "doc": "docs/jsdoc",
    "test": "js/tests"
  },
  "dependencies": {
    "websocket": "^1.0.22"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "esdoc": "^0.4.7",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.2",
    "jsdoc": "^3.4.0",
    "mocha": "^2.4.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "stage": 1
    }
  },
  "scripts": {
    "prepublish": "gulp",
    "build": "gulp",
    "test": "mocha -t 5000 --no-colors --use_strict js/tests/test.js",
    "lint": "eslint js/src/*.js",
    "esdoc": "esdoc -c esdoc.json",
    "jsdoc": "jsdoc --verbose --readme js/docs/index.md js/src/*.js -d docs/jsdoc/ -c js/docs/conf.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svenkreiss/databench.git"
  },
  "author": "@svenkreiss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svenkreiss/databench/issues"
  },
  "homepage": "http://databench.trivial.io"
}
